// A simple math utilities module for testing

// Public constants
pub var PI = 3.14159;
pub var E = 2.71828;

// Private helper variable
var precision = 6;

// Public functions
pub fn add(a, b) {
    a + b
}

pub fn multiply(a, b) {
    a * b
}

pub fn square(x) {
    multiply(x, x)
}

// Public function that uses private helper
pub fn round_to_precision(num) {
    var factor = 10.0 ** precision;
    (num * factor + 0.5) / factor
}

// Private function (not exported)
fn internal_helper() {
    "This is private"
}

// Module-level initialization
var initialized = true;
print("Math utils module loaded");