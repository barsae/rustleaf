// Comprehensive module system test covering basic import functionality

// Test namespace import
use math_utils;
assert(math_utils.PI == 3.14159);
assert(math_utils.add(2, 3) == 5);

// Test named imports with alias
use math_utils::{E, multiply, add as plus};
assert(E == 2.71828);
assert(multiply(4, 5) == 20);
assert(plus(10, 20) == 30);

