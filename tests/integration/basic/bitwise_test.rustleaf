// #[fail_quietly]
// Bitwise operators test
assert((5 & 3) == 1);  // 101 & 011 = 001
assert((5 | 3) == 7);  // 101 | 011 = 111
assert((5 ^ 3) == 6);  // 101 ^ 011 = 110

// Bit shifts
assert((8 << 1) == 16);
assert((8 >> 1) == 4);
assert((8 >> 2) == 2);

// Bitwise NOT
assert((~5) == -6);  // Two's complement