// Test nested braces in interpolation expressions
// Note: Complex dictionary access in interpolation is not fully supported yet
// Using simpler expressions that work with current implementation

// Test nested dictionary access - using simple variables
var theme = "dark";
var nested_access = "Theme: ${theme}";
assert(nested_access == "Theme: dark");

// Test function call with simple parameter
fn get_value(val) {
    val
}

var test_val = "test";
var with_func = "Data: ${get_value(test_val)}";
assert(with_func == "Data: test");